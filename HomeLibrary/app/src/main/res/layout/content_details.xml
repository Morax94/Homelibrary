<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.morax.homelibrary.DetailsActivity"
    tools:showIn="@layout/app_bar_details">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="138dp"
        android:layout_height="218dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/Title_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:enabled="false"
        android:inputType="text"
        android:singleLine="true"
        android:text="Name"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/author_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/details_autor"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="59dp" />

    <EditText
        android:id="@+id/date_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:enabled="false"
        android:inputType="date"
        android:text="Name"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="131dp" />

    <EditText
        android:id="@+id/page_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:enabled="false"
        android:inputType="numberDecimal"
        android:text="Name"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="251dp" />

    <EditText
        android:id="@+id/publiser_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:enabled="false"
        android:inputType="text"
        android:text="Name"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="193dp" />

    <EditText
        android:id="@+id/author_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:enabled="false"
        android:inputType="textPersonName"
        android:text="Name"
        android:textAppearance="@style/TextAppearance.AppCompat"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="76dp" />

    <TextView
        android:id="@+id/date_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/details_data"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="114dp" />

    <TextView
        android:id="@+id/publiser_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/details_publisher"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="176dp" />

    <TextView
        android:id="@+id/page_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/detalis_page"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="234dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/details_description"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="306dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="343dp"
        android:layout_height="169dp"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="332dp" />

</android.support.constraint.ConstraintLayout>
